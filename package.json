{
  "name": "@testingrequired/tf",
  "version": "0.0.7",
  "description": "Middlewear based test framework",
  "main": "lib/index.js",
  "bin": {
    "tf": "bin/tf"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "lib"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "precli": "npm run build",
    "cli": "node ./bin/tf",
    "cli:spec": "npm run cli -- spec",
    "cli:suite": "npm run cli -- suite",
    "cli:custom": "npm run cli -- custom",
    "prepublishOnly": "npm run test && npm run build",
    "pack-preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "jest"
  },
  "author": "Kylee Tilley <kylee@testingrequired.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "jest": "24.7.1",
    "ts-jest": "24.0.1",
    "typescript": "3.4.1"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@types/glob": "7.1.1",
    "esm": "3.2.22",
    "glob": "7.1.3",
    "junit-report-builder": "1.3.2",
    "yargs": "^13.2.2"
  }
}
