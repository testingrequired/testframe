{
  "name": "@testingrequired/tf",
  "version": "0.0.4",
  "description": "Middlewear based test framework",
  "main": "lib/index.js",
  "bin": {
    "tf": "bin/tf"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "lib"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "precli": "npm run build",
    "cli": "node ./bin/tf",
    "cli:custom": "npm run cli -- ./my-cli.js",
    "prepublishOnly": "npm run test && npm run build",
    "pack-preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "jest"
  },
  "author": "Kylee Tilley <kylee@testingrequired.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.5",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@types/glob": "^7.1.1",
    "esm": "^3.2.20",
    "glob": "^7.1.3",
    "junit-report-builder": "^1.3.2"
  }
}
