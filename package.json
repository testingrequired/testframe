{
  "name": "@testingrequired/testframe",
  "version": "0.1.0",
  "description": "A testing framework",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "precli": "npm run build",
    "cli": "node -r esm ./bin/testframe",
    "prepublishOnly": "npm run verify && npm run build",
    "pack-preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "test": "jest",
    "verify": "npm run test && npm run cli",
    "coverage": "jest --coverage"
  },
  "author": "Kylee Tilley <kylee@testingrequired.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "jest": "24.7.1",
    "ts-jest": "24.0.2",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@babel/runtime": "7.4.3",
    "@testingrequired/multiassert": "^1.0.0",
    "@types/glob": "7.1.1",
    "chance": "^1.0.18",
    "esm": "3.2.22",
    "glob": "7.1.3",
    "junit-report-builder": "1.3.2",
    "testdouble": "^3.11.0",
    "yargs": "^13.2.2"
  }
}
